<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <agent>
    <music sourceDirectory="C:\Code\m\Music"
           searchPattern="*.mp3|*.ogg"
           currentProcessingComponentKey="real"
           currentMonitoringComponentKey="realmusic" xdt:Transform="Replace"/>
    <lyrics sourceDirectory="C:\Code\m\Lyrics"
           searchPattern="*.lrc"
           currentProcessingComponentKey="real"
           currentMonitoringComponentKey="reallyrics" xdt:Transform="Replace"/>
  </agent>
</configuration>